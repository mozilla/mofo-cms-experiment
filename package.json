{
  "name": "mofo-cms-experiment",
  "version": "0.0.1",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build-webpack": "webpack --config bundle.config.js && webpack --config app.config.js",
    "eslint": "eslint --fix **/*.jsx **/*.js",
    "less": "node ./node_modules/less/bin/lessc public/less/style.less > public/style.css",
    "start": "npm run less && npm run build-webpack && node app.bundle.js",
    "dev": "npm run eslint && npm run start"
  },
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "less": "^2.6.0",
    "moment": "^2.11.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "superagent": "^1.7.1",
    "webpack": "^1.12.11"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "less-loader": "^2.2.2",
    "mofo-style": "^2.0.0",
    "webpack-dev-server": "^1.14.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "./node_modules/mofo-style/.eslintrc.yaml",
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true,
      "browser": true
    },
    "ecmaFeatures": {
      "es6": true,
      "jsx": true
    },
    "rules": {
      "no-console": 1,
      "strict": [
        2,
        "never"
      ]
    },
    "plugins": [
      "react"
    ]
  }
}
